{
    "outputDirectory": "app/dist",
    "rewrites": [
        {
            "source": "/app/(.*\\..*)",
            "destination": "/$1"
        },
        {
            "source": "/app/((?!.*\\.).*)",
            "destination": "/index.html"
        },
        {
            "source": "/app",
            "destination": "/index.html"
        }
    ]
}